package thp.tp2.actividad2.ejercicio3;

public class Vehiculo {
	private String marca;
	private String modelo;
	private String color;
	private int velMax;
	
	public Vehiculo(String marca, String modelo, String color, int velMax) {
		this.marca = marca;
		this.modelo = modelo;
		this.color = color;
		this.velMax = velMax;
	}
}